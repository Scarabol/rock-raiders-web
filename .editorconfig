[*.ts]
ij_typescript_spaces_within_imports = true
ij_typescript_use_semicolon_after_statement = false
ij_typescript_force_semicolon_style = true
ij_typescript_force_quote_style = true
ij_typescript_use_double_quotes = false
ij_typescript_keep_blank_lines_in_code = 1
ij_typescript_blank_lines_around_method_in_interface = 0

ij_typescript_array_initializer_wrap = off
ij_typescript_assignment_wrap = off
ij_typescript_binary_operation_wrap = off
ij_typescript_call_parameters_wrap = off
ij_typescript_enum_constants_wrap = off
ij_typescript_extends_keyword_wrap = off
ij_typescript_extends_list_wrap = off
ij_typescript_for_statement_wrap = off
ij_typescript_imports_wrap = off
ij_typescript_method_call_chain_wrap = off
ij_typescript_method_parameters_wrap = off
ij_typescript_object_literal_wrap = off
ij_typescript_object_types_wrap = off
ij_typescript_prefer_parameters_wrap = false
ij_typescript_ternary_operation_wrap = off
ij_typescript_union_types_wrap = off
ij_typescript_var_declaration_wrap = off
ij_typescript_wrap_comments = false
